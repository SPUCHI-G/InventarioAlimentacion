@model Inventario_Alimentacion.Models.Temporal2

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layoutjefe.cshtml";
}

<link href="~/Content/Site.css" rel="stylesheet" />
<h1 style="text-align: center">
    <strong class="title">ELIMINAR</strong>
</h1>
<h3>¿Está seguro de eliminar este registro temporal?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("ID: ")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ID_Det_salid)
        </dd>


        <dt>
            @Html.DisplayName("Categoria: ")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Producto.Categoria.Cat_Nombre)
        </dd>

        <dt>
            @Html.DisplayName("Subcategoria: ")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Producto.Subcategoria.Subca_Nombre)
        </dd>

        <dt>
            @Html.DisplayName("Producto: ")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Producto.Nom_Producto.NomProducto)
        </dd>

        <dt>
            @Html.DisplayName("Tipo de salida: ")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Tipo_Salida.Tipo_Nombre)
        </dd>

        <dt>
            @Html.DisplayName("Cantidad: ")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Sal_Cantidad)
        </dd>

        <dt>
            @Html.DisplayName("Observaciones: ")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Sal_Observacion)
        </dd>



    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-danger" /> |
            @Html.ActionLink("REGRESAR", "Create", "Detalle_Salida")
        </div>
    }
</div>
